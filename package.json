{
	"name": "json-sorter",
	"displayName": "JSON Sorter",
	"description": "VSCode extension for sort the JSON files",
	"keywords": [
		"vscode",
		"vscode-extension",
		"JSON sorter"
	],
	"version": "0.0.13",
	"icon": "assets/img/json-sorter.png",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"license": "MIT",
	"activationEvents": [
		"onCommand:extension.jsonSorter",
		"onCommand:extension.jsonSorterDesc"
	],
	"publisher": "chen86860",
	"main": "./out/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/chen86860/vscode-extension-json-sorter"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.jsonSorter",
				"title": "JSON sorter ASC"
			},
			{
				"command": "extension.jsonSorterDesc",
				"title": "JSON sorter DESC"
			}
		],
		"keybindings": {
			"command": "extension.jsonSorter",
			"key": "ctrl+shift+j",
			"mac": "cmd+shift+j",
			"when": "editorTextFocus"
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn",
		"package": "vsce package --yarn"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jju": "^1.4.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vsce": "^1.74.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"jju": "^1.4.0"
	}
}
